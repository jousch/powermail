<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<!--  <langChildren>1</langChildren>!-->
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<name>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.name</label>
					<config>
						<type>input</type>
						<size>35</size>
						<max>20</max>
						<eval>required,trim,lower,alphanum_x,nospace</eval>
					</config>
				</TCEforms>
			</name>            
			<options>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.options</label>
					<config>
						<type>text</type>
						<cols>30</cols>
						<rows>5</rows>
					</config>
				</TCEforms>
			</options>
			<mandatory>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.mandatory</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</mandatory>
			<example>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.example</label>
					<config>
						<type>user</type>
						<userFunc>tx_powermail_tx_powermail_example->main</userFunc>
						<field>radio</field>
					</config>
				</TCEforms>
			</example>
		</el>
	</ROOT>
</T3DataStructure>